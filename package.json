{
  "name": "@silexlabs/silex-dashboard",
  "version": "1.0.41",
  "description": "Dashboard for Silex v3",
  "main": ".silex.js",
  "scripts": {
    "start": "concurrently --kill-others \"$npm_execpath run silex:dev\" \"$npm_execpath run 11ty:dev\" \"$npm_execpath run tina:dev\"",
    "silex:dev": "silex --cors-url * --fs-root `pwd`/_silex --fs-hosting-root `pwd`/_published --server-config `pwd`/silex/server-config.js --client-config `pwd`/silex/client-config.js --debug",
    "11ty:dev": "eleventy --serve --config=eleventy.config.mjs",
    "11ty:build": "eleventy",
    "tina:dev": "tinacms dev",
    "tina:build": "tinacms build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/silexlabs/silex-dashboard.git"
  },
  "keywords": [
    "silex",
    "plugin"
  ],
  "author": "lexoyo",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/silexlabs/silex-dashboard/issues"
  },
  "homepage": "https://github.com/silexlabs/silex-dashboard#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0-alpha.5",
    "@silexlabs/silex": "^3.0.0-alpha.149",
    "@silexlabs/silex-cms": "^0.0.98",
    "@tinacms/cli": "^1.5.43",
    "@types/node": "^20.12.5",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "locale": "^0.1.0",
    "node_modules-path": "^2.0.7",
    "serve-static": "^1.15.0",
    "vue": "^3.4.21"
  }
}
