{
  "name": "@silexlabs/silex-dashboard",
  "version": "1.0.41",
  "description": "Dashboard for Silex v3",
  "main": ".silex.js",
  "scripts": {
    "start": "silex --server-config `pwd`/.silex.js --cors-url * --fs-root `pwd`/_silex --fs-hosting-root `pwd`/_published --client-config `pwd`/.silex-client.js --debug",
    "build": "eleventy",
    "serve": "eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/silexlabs/silex-dashboard.git"
  },
  "keywords": [
    "silex",
    "plugin"
  ],
  "author": "lexoyo",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/silexlabs/silex-dashboard/issues"
  },
  "homepage": "https://github.com/silexlabs/silex-dashboard#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@silexlabs/silex": "^3.0.0-alpha.149",
    "@silexlabs/silex-cms": "^0.0.94"
  },
  "dependencies": {
    "locale": "^0.1.0",
    "node_modules-path": "^2.0.7",
    "serve-static": "^1.15.0",
    "vue": "^3.4.21"
  }
}
