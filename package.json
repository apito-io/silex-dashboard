  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@silexlabs/silex": "^3.0.0-alpha.31"
  },
  "dependencies": {
    "locale": "^0.1.0",
    "serve-static": "^1.15.0"
